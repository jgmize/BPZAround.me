{% extends "bpz/base.jinja2" %}

{% block content %}

  <divlass="panel panel-default">
    <div class="panel-heading">Home Owners Associations</div>
    <ul id="case-list" class="list-group"></ul>
  </div>



{% endblock content %}



{% block body_js -%}
  <script>
  $(document).ready(function () {
        bpz.activateMap()  
        bpz.api.hoa().done(function (data) {
          bpz.updateMap(data)
          bpz.lists.build ("#case-list",data)
          bpz.lists.append.hoa("#case-list")
        }) //end get data
    })//end doc ready
  </script>
{%- endblock body_js %}  
  



